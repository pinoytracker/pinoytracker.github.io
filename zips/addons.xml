<addon id="script.module.beautifulsoup4" name="beautifulsoup4" version="4.12.2" provider-name="Leonard Richardson">
  <requires>
    <import addon="xbmc.python" version="3.0.0"/>
    <import addon="script.module.soupsieve" version="2.4.1"/>
  </requires>
  <extension point="xbmc.python.module" library="lib" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en_GB">Screen-scraping library</summary>
    <description lang="en_GB">Beautiful Soup is a library that makes it easy to scrape information from web pages. It sits atop an HTML or XML parser, providing Pythonic idioms for iterating, searching, and modifying the parse tree.</description>
    <license>MIT</license>
    <platform>all</platform>
    <website>https://www.crummy.com/software/BeautifulSoup/</website>
	<source>https://code.launchpad.net/beautifulsoup</source>
    <assets>
      <icon>resources/icon.png</icon>
    </assets>
  </extension>
</addon>

<addon id="script.module.requests"
       name="requests"
       version="2.22.0"
       provider-name="kennethreitz">
  <requires>
    <import addon="xbmc.python" version="2.25.0"/>
    <import addon="script.module.urllib3" version="1.25.6"/>
    <import addon="script.module.chardet" version="3.0.4"/>
    <import addon="script.module.idna" version="2.8"/>
    <import addon="script.module.certifi" version="2019.9.11"/>
  </requires>
  <extension point="xbmc.python.module"
             library="lib" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en_GB">Python HTTP for Humans</summary>
    <description lang="en_GB">Packed for KODI from https://pypi.org/project/requests/</description>
    <platform>all</platform>
    <language></language>
    <license>Apache2</license>
    <forum></forum>
    <website>http://python-requests.org</website>
    <source>https://github.com/Freso/script.module.requests</source>
    <email>freso.dk AT gmail.com</email>
    <assets>
      <icon>resources/icon.png</icon>
    </assets>
  </extension>
</addon>
