<addons>
<addon id="plugin.airtable.sync"
       name="Airtable Sync Background"
       version="1.0.0"
       provider-name="YourName">
    <requires>
        <import addon="xbmc.python" version="3.0.0"/>
        <import addon="script.module.requests" version="2.22.0"/>
        <import addon="script.module.beautifulsoup4" version="4.12.2"/>
    </requires>
    <extension point="xbmc.service" library="service.py" start="startup">
        <provides>executable</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Background Airtable Sync</summary>
        <description>Scrapes episodes and syncs to Airtable every hour.</description>
        <platform>all</platform>
    </extension>
</addon>

